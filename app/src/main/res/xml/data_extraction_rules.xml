<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- TODO: Use <include> and <exclude> to control what is backed up.
        Currently, only device encrypted storage is supported for Wi-Fi security data.
        -->
        <include domain="device_encrypted" path="."/>
    </cloud-backup>
    <!-- No roaming backup for security-sensitive data -->
    <device-transfer>
        <include domain="device_encrypted" path="."/>
    </device-transfer>
</data-extraction-rules>