<full-backup-content>
    <!-- Include preferences but exclude sensitive data -->
    <include domain="sharedpref" path="."/>
    <exclude domain="database" path="."/> <!-- Don't backup database with security info -->
    <exclude domain="file" path="encryption_keys/"/> <!-- Exclude encryption key files -->
</full-backup-content>